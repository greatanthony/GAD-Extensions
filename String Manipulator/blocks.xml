<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<palette>




  <block     tag="GAD-STP-getfirstxchars" 
         spec="get first %0 characters of %1  "            
         code="STP.getfirstxchars(~,~)"
         help="gets the first desired characters of the string.(space counts)"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
		<number order="0"/>
           <text order="1"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-getlastxchars" 
         spec="get last %0 characters of %1  "            
         code="STP.getlastxchars(~,~)"
         help="gets the last desired characters of the string.(space counts)"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
		<number order="0"/>
           <text order="1"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-elimspace" 
         spec=" %0 with all spaces removed "            
         code="STP.eliminatespace(~)"
         help="returns the text but with all spaces removed"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
           <text order="0"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-toupper" 
         spec=" %0 as Uppercased text "            
         code="STP.toupper(~)"
