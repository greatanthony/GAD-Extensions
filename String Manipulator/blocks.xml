	<block	tag="GAD-STP-chchar"
		spec="currentchar"
		code="chchar"
		help="Use inside the for chchar block"
		type="normal"
		color="blue"
		hidden="true"
		returns="text">
		<fields/>
	</block>
	
	<block	tag="GAD-STP-iteratestring"
		spec="for %2 in %0"
		code="STP.iterate(~,function(chchar:String) {~});"
		help="repeats the inner code with currentchar looping through each character of the text"
		type="wrapper"
		color="yellow"
		returns="void">
		<fields>
			<text order="0"/>
			<CODE_BLOCK order="1"/>
			<attached-block order="2" tag="GAD-STP-chchar"/>
		</fields>
	</block>


  <block     tag="GAD-STP-ord" 
         spec="ordinal value of %0"            
         code="STP.ord(~)"
         help="Returns the character code(ordinal value) of the given Single Character"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="number">
    <fields>
           <text order="0"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-chr" 
         spec="character of ordinal value %0"            
         code="STP.chr(~)"
         help="Returns the character of the given ordinal value"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
           <number order="0"/>
  </fields>
  </block>	













</palette>
