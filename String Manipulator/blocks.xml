         color="lime" 
         hidden="false"
         returns="number">
    <fields>
           <text order="0"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-upcheck" 
         spec=" is %0 all uppercase letters?"            
         code="STP.upcheck(~)"
         help="Determines whether the text is all uppercase letters."
