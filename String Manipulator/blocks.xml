  </block>	


  <block     tag="GAD-STP-splitbw" 
         spec="text between %0 and %1 in %2"            
         code="STP.splitbw(~,~,~)"
         help="returns the first text content occurence between the two text inputs in the third text"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
           <text order="0"/>
           <text order="1"/>
           <text order="2"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-splitbwlist" 
         spec="enlist all text between every %0 and %1 in %2"            
         code="STP.splitbwlist(~,~,~)"
         help="splits the text using the the two delimter texts in a list."
         type="normal" 
         color="lime" 
         hidden="true"
         returns="list">
    <fields>
           <text order="0"/>
           <text order="1"/>
           <text order="2"/>
  </fields>
  </block>	


  <block     tag="GAD-STP-nocurence" 
         spec="no. of occurence of %0 in %1"            
         code="STP.nocurence(~,~)"
         help="returns the number of times the first text appears in the second text"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="list">
    <fields>
           <text order="0"/>
           <text order="1"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-digcheck" 
         spec=" is %0 all digits?"            
         code="STP.digcheck(~)"
         help="Determines whether the text is all digits."
