         hidden="false"
         returns="boolean">
    <fields>
           <text order="0"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-alphacheck" 
         spec=" is %0 all alphabets?"            
         code="STP.alphacheck(~)"
