  <block     tag="GAD-STP-exists" 
         spec=" %0 exists in %1 "            
         code="STP.exists(~,~)"
         help="Determines whether the first text exists in the second text or not."
         type="normal" 
         color="lime" 
         hidden="false"
         returns="boolean">
    <fields>
           <text order="0"/>
           <text order="1"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-lenth" 
         spec="no. of characters in %0 "            
         code="STP.len(~)"
         help="Returns number of characters in the text"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="number">
    <fields>
           <text order="0"/>
  </fields>
  </block>	


  <block     tag="GAD-STP-xremovey" 
         spec=" %0 with %1 removed from it "            
         code="STP.xremovey(~,~)"
         help="Returns the first text but with second one removed from it, if it exists in it."
         type="normal" 
         color="lime" 
         hidden="false"
         returns="number">
    <fields>
           <text order="0"/>
           <text order="1"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-rev" 
         spec=" %0 reversed"            
