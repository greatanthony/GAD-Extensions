         type="normal" 
         color="lime" 
         hidden="false"
         returns="boolean">
    <fields>
           <text order="0"/>
  </fields>
  </block>	

  <block     tag="GAD-STP-repeat" 
         spec=" %0 x %1"            
         code="STP.repeat(~,~)"
         help="returns the string by replicating the string by the second number times"
         type="normal" 
         color="lime" 
         hidden="false"
         returns="text">
    <fields>
           <text order="0"/>
           <number order="1"/>
  </fields>
  </block>	


  <block     tag="GAD-STP-spacecheck" 
         spec=" is %0 all space?"            
         code="STP.spacecheck(~)"
         help="Determines whether the text is all spaces."
         type="normal" 
         color="lime" 
         hidden="false"
         returns="boolean">
